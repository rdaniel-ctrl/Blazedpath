    "parameters": [
      {
        "bind": "quizId",
        "in": "Path",
        "name": "quizId",
        "remarks": "Id."
      },
      {
        "bind": "quiz",
        "in": "Body",
        "name": "body",
        "remarks": "New Quiz information to be updated."
      }
    ],
    "path": "/quiz/{quizId}",
    "remarks": "Allows to update an existing Quiz.",
    "responses": []
  }
}